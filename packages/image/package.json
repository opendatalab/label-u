    "lodash.debounce": "^4.0.8",
    "lodash.throttle": "^4.1.1",
    "rbush": "^3.0.1",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/color": "^3.0.6",
    "@types/lodash.clonedeep": "^4.5.9",
    "@types/rbush": "^3.0.3",
    "@types/uuid": "^9.0.0",
    "esbuild": "^0.11.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.43.1",
    "rollup-plugin-esbuild": "^3.0.2",
    "typescript": "4.8.4"
  }
