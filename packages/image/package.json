    "clean": "rimraf ./node_modules",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@labelu/interface": "workspace:1.2.1",
    "color": "^4.2.3",
    "eventemitter3": "^5.0.0",
