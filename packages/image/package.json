{
  "name": "@labelu/image",
  "version": "1.0.1",
  "description": "Image annotation tool for labelU",
  "author": {
    "name": "GaryShen",
    "email": "grinson50@gmail.com"
  },
  "license": "Apache-2.0",
  "types": "dist/types/index.d.ts",
  "typings": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/types/index.d.ts",
        "default": "./es/index.js"
      },
      "require": {
        "types": "./dist/types/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "homepage": "",
  "main": "dist/index.js",
  "module": "es/index.js",
  "files": [
    "lib",
    "es"
  ],
